	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "Launch hello-world",
			"type": "node",
			"program": "${workspaceFolder}/examples/hello-world/server.ts",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "/home/grenierdev/.deno/bin/deno",
			"env": {
				"DENO_DIR": ".deno"
			},
			"runtimeArgs": [
				"run",
				"--unstable",
				"--allow-env",
				"--allow-read",
				"--allow-net",
				"--allow-sys",
				"--inspect"
			],
			"attachSimplePort": 9229,
			"restart": true
		}
	]
}
